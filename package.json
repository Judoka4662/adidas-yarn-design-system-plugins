{
  "name": "@adidas/yarn-design-system-plugins",
  "version": "1.0.1",
  "description": "adidas YARN Design System plugins",
  "repository": {
    "type": "git",
    "url": "https://github.com/adidas/adidas-yarn-design-system-plugins.git"
  },
  "license": "MIT",
  "contributors": [
    "David Recuenco <david.recuencogadea@gmail.com>",
    "Jaime Pastor <moelders@gmail.com>"
  ],
  "homepage": "http://adidas.github.io/adidas-yarn-design-system-plugins/",
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "scripts": {
    "setup": "npm run lerna:init && npm run lerna:bootstrap",
    "start": "nuxt",
    "clean": "npm run clean:example && lerna exec -- npm run clean",
    "clean:example": "del docs && del .nuxt",
    "build": "lerna exec -- npm run build",
    "build:example": "nuxt generate",
    "build:example:spa": "cross-env NODE_ENV=production nuxt generate --spa",
    "lint": "npm run lint:example && lerna exec -- npm run lint",
    "lint:example": "eslint --ext .js,.vue example && stylelint example/**/*.{less,vue}",
    "doc": "npm run build:example:spa",
    "lerna:init": "lerna init",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:exec": "lerna exec",
    "lerna:publish": "lerna exec -- safe-publish"
  },
  "devDependencies": {
    "@adidas/yarn-design-system": "1.5.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "7.1.4",
    "babel-plugin-istanbul": "4.1.6",
    "babel-preset-env": "1.7.0",
    "choices.js": "4.1.3",
    "choicesjs-stencil": "1.2.1",
    "copy-webpack-plugin": "4.5.2",
    "cross-env": "5.1.1",
    "del-cli": "1.1.0",
    "eslint": "5.10.0",
    "eslint-config-adidas-babel": "1.1.0",
    "eslint-config-adidas-env": "1.1.0",
    "eslint-config-adidas-es8": "1.1.0",
    "eslint-config-adidas-vue": "1.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-vue": "5.0.0",
    "expose-loader": "0.7.5",
    "extract-loader": "2.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "lerna": "^2.11.0",
    "less": "3.8.1",
    "less-loader": "4.1.0",
    "nuxt": "1.4.2",
    "postcss-loader": "2.1.6",
    "raw-loader": "0.5.1",
    "safe-publish": "1.0.0",
    "source-map-loader": "0.2.4",
    "stylelint": "9.9.0",
    "stylelint-config-adidas": "1.2.1",
    "stylelint-config-adidas-bem": "1.2.0",
    "typeface-poppins": "0.0.54",
    "typeface-roboto": "0.0.54",
    "typeface-roboto-mono": "0.0.54",
    "vue-i18n": "7.6.0",
    "vue-showdown": "2.0.2",
    "webpack": "3.8.1"
  },
  "keywords": [
    "yarn",
    "design",
    "design-system",
    "plugins"
  ]
}
